{
    "name": "UTC TV",
    "description": "Manipulação da tela WOD TV do aplicativo Tecnofit",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "/images/icon-32x32.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://app.tecnofit.com.br/*"],
            "js": ["js/jquery-3.6.0.min.js", "js/contentScriptNew.js"]
        }
    ],
    "icons": {
        "32": "/images/icon-32x32.png"
    }
}